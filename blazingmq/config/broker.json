{
  "appConfig": {
    "brokerInstanceName": "exousia-broker",
    "brokerVersion": 999999,
    "configVersion": 1,
    "hostName": "blazingmq-broker",
    "hostTags": ["laptop"],
    "hostDataCenter": "local",
    "isRunningOnDev": true,
    "logsObserverMaxSize": 2000,
    "dispatcherConfig": {
      "sessions": {
        "heartbeatIntervalMs": 3000
      },
      "queues": {
        "allocator": {
          "type": "COUNTING"
        }
      }
    },
    "stats": {
      "snapshotInterval": 60,
      "appIdTagDomains": ["exousia"],
      "plugins": [
        {
          "name": "prometheus",
          "publishInterval": 30,
          "port": 9091
        }
      ]
    },
    "networkInterfaces": {
      "tcpInterface": {
        "name": "tcp",
        "port": 30114,
        "ioThreads": 4,
        "maxConnections": 1000
      },
      "heartbeats": {
        "client": 30,
        "downstreamBroker": 60,
        "upstreamBroker": 60,
        "cluster": 30
      }
    }
  },
  "domainConfig": {
    "domains": [
      {
        "name": "exousia",
        "mode": "priority",
        "storage": {
          "domainLimits": {
            "messages": 10000000,
            "bytes": 10737418240
          },
          "queueLimits": {
            "messages": 1000000,
            "bytes": 1073741824
          },
          "config": {
            "clusterName": "local",
            "fileStore": {
              "location": "/var/lib/blazingmq/storage"
            }
          }
        },
        "queues": [
          {
            "name": "build.queue",
            "mode": "priority",
            "maxDeliveryAttempts": 5,
            "deduplicationTimeMs": 300000,
            "consistencyLevel": "strong",
            "subscriptions": []
          },
          {
            "name": "build.dlq",
            "mode": "fanout",
            "maxDeliveryAttempts": 1,
            "consistencyLevel": "strong",
            "subscriptions": []
          },
          {
            "name": "build.status",
            "mode": "priority",
            "maxDeliveryAttempts": 3,
            "deduplicationTimeMs": 60000,
            "consistencyLevel": "eventual",
            "subscriptions": []
          }
        ]
      }
    ]
  },
  "clusters": [
    {
      "name": "local",
      "nodes": [
        {
          "id": 1,
          "name": "node1",
          "datacenter": "local",
          "transport": {
            "tcp": {
              "endpoint": "tcp://blazingmq-broker:30114"
            }
          }
        }
      ],
      "partitionConfig": {
        "numPartitions": 4,
        "location": "/var/lib/blazingmq/clusters/local"
      },
      "masterAssignment": {
        "type": "E_LEADER_IS_MASTER_ALL"
      },
      "elector": {
        "initialWaitTimeoutMs": 5000,
        "maxRandomWaitTimeoutMs": 10000,
        "scoutingResultTimeoutMs": 30000,
        "electionResultTimeoutMs": 30000,
        "heartbeatBroadcastPeriodMs": 1000,
        "heartbeatCheckPeriodMs": 2000,
        "heartbeatMissCount": 3,
        "quorum": 1,
        "leaderSyncDelayMs": 1000
      },
      "queueOperations": {
        "openTimeoutMs": 300000,
        "configureTimeoutMs": 300000,
        "closeTimeoutMs": 300000,
        "ackTimeoutMs": 30000,
        "confirmTimeoutMs": 30000,
        "keepaliveDurationMs": 1800000,
        "consumptionMonitorPeriodMs": 30000
      },
      "clusterAttributes": {
        "isCSLModeEnabled": false,
        "isFSMWorkflow": false
      },
      "clusterMonitorConfig": {
        "maxTimeLeader": 300,
        "maxTimeMaster": 300,
        "maxTimeNode": 300,
        "maxTimeAvailable": 300,
        "thresholdLeader": 80,
        "thresholdMaster": 80,
        "thresholdNode": 80,
        "thresholdAvailable": 80
      }
    }
  ]
}
